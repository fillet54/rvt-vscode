{
	"name": "rvt",
	"displayName": "rvt-vscode",
	"description": "Language support for RVT",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.35.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld",
		"onLanguage:rvt"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		],
		"languages": [
			{
				"id": "rvt",
				"extensions": [
					".rvt"
				],
				"aliases": [
					"RVT",
					"rvt"
				]
			}
		],
		"grammars": [
			{
				"language": "rvt",
				"scopeName": "source.rvt",
				"path": "./syntaxes/rvt.tmGrammar.json"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"eslint": "^5.13.0",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}